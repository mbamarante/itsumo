dist: 
	@mkdir -p .tmp/witsumo && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/witsumo/ && $(COPY_FILE) --parents images/generic.png .tmp/witsumo/ && $(COPY_FILE) --parents mainwindow.ui.h .tmp/witsumo/ && ( cd `dirname .tmp/witsumo` && $(TAR) witsumo.tar witsumo && $(GZIP) witsumo.tar ) && $(MOVE) `dirname .tmp/witsumo`/witsumo.tar.gz . && $(DEL_FILE) -r .tmp/witsumo

