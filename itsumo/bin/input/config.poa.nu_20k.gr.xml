<config>
 <file>input/net.poa.notl.xml</file>
 <odmatrix>input/demand.poa.nu_30k.xml</odmatrix>
 <odsize>20000</odsize>
 <steps>15000</steps>
 <default_deceleration>0</default_deceleration>
 <sensor_interval>20</sensor_interval>
 <agent_interval>600</agent_interval>
 <car_max_speed>3</car_max_speed>
 <cell_size>5</cell_size>
 <iteration_length>1</iteration_length>
 <drivers></drivers>
 <rounds>1</rounds>
 <episodes>1</episodes>
 <costs_view>GLOBAL</costs_view>
 <replan>1</replan>
 <communication>0</communication>
 <sensors>
  <sensor>
   <name>avg_travel_times</name>
   <file>../../output/sensor.poa.nu_20k.gr.avg_travel_times.txt</file>
   <state>ON</state>
   <time_tag>NO</time_tag>
  </sensor>
  <sensor>
   <name>travel_times</name>
   <file>../../output/sensor.poa.nu_20k.gr.travel_times.txt</file>
   <state>ON</state>
   <time_tag>NO</time_tag>
  </sensor>
  <sensor>
   <name>laneset_densities_sensor</name>
   <file>../../output/sensor.poa.nu_20k.gr.laneset_densities_sensor.txt</file>
   <state>ON</state>
   <time_tag>NO</time_tag>
  </sensor>
 </sensors>
</config>
